{
  "name": "101-vue-cli-project",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name 101-vue-cli-project --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "prepublishOnly": "npm run lint && npm run build && npm run styleguide:build && npm run doc:build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "doc:build": "npx vuedoc.md --section API --output ./README.md ./src/components/component.vue"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-prettier": "^3.0.0-rc.10",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuedoc/md": "^1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "lint-staged": "^7.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-component": "^1.6.6",
    "vue-cli-plugin-vuepress": "^0.1.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.1.1",
    "vue-styleguidist": "^1.7.13",
    "vue-template-compiler": "^2.5.16"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "dist/101-vue-cli-project.umd.js",
  "module": "dist/101-vue-cli-project.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
